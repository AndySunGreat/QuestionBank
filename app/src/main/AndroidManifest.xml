<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aladdin.apps.questionbank">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppBaseTheme">
        <activity
            android:name=".login.LoginActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".base.BaseActivity"></activity>

        <activity
            android:name=".login.signup.RegisterActivity"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name=".home.HomeActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"
            android:logo="@drawable/ic_pets_black_24dp"
            >
        </activity>

        <activity
            android:name=".discovery.DiscoveryMainActivity"
            android:label="@string/app_name">
        </activity>

        <!-- "题库" 频道 -->
        <activity
            android:name=".question.bank.QuestionModuleBankActivity"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name=".question.bank.QuestionFeatureBankActivity"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name=".question.boss.QuestionFeatureBossActivity"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name=".question.hot.QuestionFeatureHotActivity"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name=".question.salary.QuestionModuleSalaryActivity"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name=".question.strategy.QuestionModuleStrategyActivity"
            android:label="@string/app_name">
        </activity>
        <!-- "我" 频道 -->
        <activity
            android:name=".me.MeFeatureInfoActivity"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name=".me.MeModuleAssetActivity"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name=".me.MeModuleInfoActivity"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name=".me.MeModulePlanActivity"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name=".me.MeModuleSettingsActivity"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name=".me.MeModuleBankActivity"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name=".me.MeModuleTargetActivity"
            android:label="@string/app_name">
        </activity><!-- ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>